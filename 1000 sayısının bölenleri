using System;

namespace OrnekUygulama
{
    class Program
    {
        static void Main(string[] args)
        {
            int i = 1;
            while (i <= 1000)
            {
                if (1000 % i == 0)
                {
                    Console.WriteLine(i);
                }
                i++;
            }
        }
    }
}
