using System;

namespace OrnekUygulama
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 1;
            int b = 1;
            Console.Write("Gösterilecek Fibonacci Adım Sayısını Girin: ");
            int sayi = int.Parse(Console.ReadLine());

            int i = 0;
            while (i < sayi)
            {
                Console.Write(a);
                int adim = a + b;
                a = b;
                b = adim;
                i++;
            }
        }
    }
}
